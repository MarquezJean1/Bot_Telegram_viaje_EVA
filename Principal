    today = date.today()
                            #asientos,costo
    auxBoleto = ['origen','destino',0,0,'Fecha i','Fecha r','nombre','cedula','cuenta bancaria']
    query = update.callback_query
    # recupera la informacion
    query.answer()
    #Toma la informacion del boton
    choose = query.data
    
    #Compara los valores del boton
    if choose == 'listar': # primero elegir el aeropuerto
      query.edit_message_text("Donde desea verificar vuelos dispoonibles", reply_markup=InlineKeyboardMarkup([
                                              [InlineKeyboardButton('Guayas, Ecuador', callback_data='Guayas')],
                                              [InlineKeyboardButton('Quito, Ecuador', callback_data='Quito')],
                                              [InlineKeyboardButton("Regresar", callback_data='regresar') ],
                                        ]))
    if choose == 'Guayas':  
      query.edit_message_text("(Guayas) Elija el continente:", reply_markup=InlineKeyboardMarkup([
                                              [InlineKeyboardButton('Africa', callback_data='gAfrica')],
                                              [InlineKeyboardButton("America", callback_data='gAmerica') ],
                                              [InlineKeyboardButton("Asia", callback_data='gAsia') ],
                                              [InlineKeyboardButton("Europa", callback_data='gEuropa') ],
                                              [InlineKeyboardButton("Oceanía", callback_data='gOceania') ],
                                              [InlineKeyboardButton("Nacionales", callback_data='gNacional') ],
                                              [InlineKeyboardButton("Regresar", callback_data='listar') ],
                                        ]))
