def main() -> None:
    # Create the Updater and pass it your bot's token.
    updater = Updater("1694478909:AAEK2bQeVpEuG961ibFH56tacOL1t265ntg")

    updater.dispatcher.add_handler(CommandHandler('start', start))
    updater.dispatcher.add_handler(CallbackQueryHandler(button)) # Controla los botones
    updater.dispatcher.add_handler(CommandHandler('help', help_command))

    # Start the Bot
    updater.start_polling()

    # Ejecute el bot hasta que el usuario presione Ctrl-C o el proceso reciba SIGINT,
    # SIGTERM o SIGABRT
    updater.idle()

##########################################
if __name__ == '__main__':
    main()
